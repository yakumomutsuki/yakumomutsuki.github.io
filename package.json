{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --watch",
    "build": "next build && next export",
    "serve": "yarn build && serve ./out",
    "lint": "next lint --dir src && markuplint **/*.tsx",
    "format": "next lint --dir src --fix && prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json}'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test-storybook": "test-storybook",
    "scaffold": "scaffdog generate",
    "contentful-typescript-codegen": "CONTENTFUL_MANAGEMENT_API_ACCESS_TOKEN=xxxxx CONTENTFUL_SPACE_ID=xxxxx CONTENTFUL_ENVIRONMENT=xxxxx contentful-typescript-codegen --output src/pages/api/contentful/codegen/contentful.d.ts"
  },
  "dependencies": {
    "bulma": "^0.9.4",
    "contentful": "^9.1.32",
    "markdown-to-jsx": "^7.1.7",
    "next": "12.1.6",
    "next-export-i18n": "^2.0.3",
    "prismjs": "^1.28.0",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@markuplint/jsx-parser": "^2.4.1",
    "@markuplint/react-spec": "^2.1.0",
    "@storybook/addon-actions": "^6.5.8",
    "@storybook/addon-essentials": "^6.5.8",
    "@storybook/addon-interactions": "^6.5.8",
    "@storybook/addon-links": "^6.5.8",
    "@storybook/builder-webpack5": "^6.5.8",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack5": "^6.5.8",
    "@storybook/react": "^6.5.8",
    "@storybook/test-runner": "^0.1.1",
    "@storybook/testing-library": "^0.0.12",
    "@types/markdown-to-jsx": "^7.0.1",
    "@types/node": "17.0.40",
    "@types/prismjs": "^1.26.0",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-jsx-remove-data-test-id": "^3.0.0",
    "contentful-management": "^10.6.3",
    "contentful-typescript-codegen": "^3.2.3",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-storybook": "^0.5.12",
    "jest": "27",
    "markuplint": "^2.8.0",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.18.1",
    "prettier": "^2.6.2",
    "scaffdog": "^1.5.0",
    "serve": "^13.0.2",
    "storybook": "^6.5.8",
    "typescript": "4.7.3"
  },
  "volta": {
    "node": "18.3.0",
    "yarn": "1.22.18"
  }
